function fish_user_abbreviations
	set -U fish_user_abbreviations \
			'g=git' \
			'ga=git add' \
			'gaa=git add -A' \
			'gp=git push' \
			'gpf=git push -f' \
			'gb=git branch' \
			'gco=git checkout' \
			'gcb=git checkout -b' \
			'gcom=git checkout master' \
			'gcbf=gcfb' \
			'gs=git status -sb' \
			'gh=git head' \
			'gl=git l' \
			'gll=git ll' \
			'gc=git commit -v' \
			'gcm=git commit -m' \
			'gf=git fetch' \
			'gfa=git fetch --all' \
			'gm=git merge' \
			'gr=git rebase' \
			'gcp=git cherry-pick' \
			'gpl=git pull' \
			'gpr=git pull-request' \
			'grpo=git remote prune origin' \
			'grc=git recommit'
end
