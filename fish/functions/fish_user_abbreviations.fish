function fish_user_abbreviations
	set -U fish_user_abbreviations \
			'g=git' \
			'gcbf=gcfb' \
			'gh=git head' \
			'gl=git l' \
			'gll=git ll' \
			'gc=git commit -v' \
			'gcm=git commit -m' \
			'gf=git fetch' \
			'gfa=git fetch --all' \
			'gm=git merge' \
			'gr=git rebase' \
			'gcp=git cherry-pick' \
			'grpo=git remote prune origin'
end
